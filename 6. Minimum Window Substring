from collections import Counter

def min_window(s,t):
    count = Counter(t)
    required = len(count)
    l,r=0,0
    formed=0
    window_counts={}
    ans=(float("inf"),None,None)
    
    while r<len(s):
        c=s[r]
        window_counts[c]=window_counts.get(c,0)+1
        if c in count and window_counts[c]==count[c]:
            formed+=1
        while l<=r and formed==required:
            if r-l+1<ans[0]:
                ans=(r-l+1,l,r)
            window_counts[s[l]]-=1
            if s[l] in count and window_counts[s[l]]<count[s[l]]:
                formed-=1
            l+=1
        r+=1
    return "" if ans[0]==float("inf") else s[ans[1]:ans[2]+1]

print(min_window("ADOBECODEBANC","ABC"))
