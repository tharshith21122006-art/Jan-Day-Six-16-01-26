from collections import defaultdict,deque

def alien_order(words):
    g=defaultdict(set); indegree=defaultdict(int)
    for i in range(len(words)-1):
        w1,w2=words[i],words[i+1]
        for c1,c2 in zip(w1,w2):
            if c1!=c2:
                if c2 not in g[c1]:
                    g[c1].add(c2)
                    indegree[c2]+=1
                break
    q=deque([c for c in set(''.join(words)) if indegree[c]==0])
    res=[]
    while q:
        c=q.popleft(); res.append(c)
        for nei in g[c]:
            indegree[nei]-=1
            if indegree[nei]==0: q.append(nei)
    return ''.join(res) if len(res)==len(set(''.join(words))) else ''

print(alien_order(["wrt","wrf","er","ett","rftt"]))
